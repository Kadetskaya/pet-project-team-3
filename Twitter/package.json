{
    "name": "landing",
    "version": "1.0.0",
    "scripts": {
        "start": "gulp",
        "clean": "gulp clean",
        "build": "gulp build",
        "test": "nyc mocha --opts ./mocha.opts"
    },
    "dependencies": {
        "@babel/core": "7.8.7",
        "@babel/preset-env": "7.8.7",
        "browser-sync": "2.26.7",
        "del": "5.1.0",
        "gulp": "4.0.2",
        "gulp-babel": "8.0.0",
        "gulp-concat": "2.6.1",
        "gulp-copy": "4.0.1",
        "gulp-debug": "4.0.0",
        "gulp-file-include": "2.1.1",
        "gulp-sass": "4.0.2",
        "gulp-sourcemaps": "2.6.5",
        "gulp-uglify": "3.0.2",
        "prettier": "1.19.1",
        "pump": "latest",
        "through2": "3.0.1",
        "util": "0.12.2",
        "vinyl-named": "1.1.0",
        "webpack": "4.42.0",
        "webpack-cli": "3.3.11",
        "webpack-stream": "5.2.1"
    },
    "nyc": {
        "extension": [
            ".js"
        ],
        "check-coverage": true,
        "per-file": true,
        "lines": 100,
        "statements": 100,
        "functions": 100,
        "branches": 100,
        "reporter": [
            "html",
            "text",
            "text-summary"
        ],
        "require": [
            "@babel/register"
        ],
        "exclude": [
            "coverage/**",
            "tests/**",
            "test{,-*}.js",
            "**/*.test.js",
            "**/__tests__/**",
            "**/node_modules/**",
            ".nyc_output/**",
            ".nyc_output-tmp/**",
            "**/mockData/**",
            "testConfig/**",
            "**/mock{,*}.js"
        ],
        "sourceMap": false,
        "cache": false,
        "all": false,
        "report-dir": "coverage"
    }
}
